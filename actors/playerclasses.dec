actor IsCT : Inventory { Inventory.MaxAmount 1 }
actor IsDead : Inventory { Inventory.MaxAmount 1 }
actor Crouch : Inventory { Inventory.MaxAmount 1 }
actor CST : DoomPlayer 
{
	Player.ViewHeight 43
	MaxStepHeight 22
    LimitedToTeam 1
	//-NOSKIN
	+NOTELESTOMP
	+USEDAMAGEEVENTSCRIPT
	var int user_headRadius;
	var int user_headMaxHeight;
	var int user_headMinHeight;
	var int user_legRadius;
	var int user_legMaxHeight;
	var int user_legMinHeight;
	Player.DisplayName "Terrorist"
	GibHealth -2147483647
	DamageFactor SmokeG, 0
	DamageFactor Wallbang, 1
	Speed 0.5
	Health 100
	Player.StartItem "Sidearm"
	Player.StartItem "HasSecondary"
	Player.StartItem "GlockAmmo" 20
	Player.StartItem "GlockReserve" 120
	Player.StartItem "CSKnife"
	Player.StartItem "Backpack"
	Player.StartItem "MoneyGive"
	Player.MaxHealth 100
	Player.SoundClass "cas"
  States
  {
  Spawn:
	PXCN A 0 NoDelay A_SetUserVar("user_headRadius", 10.0)
	PXCN A 0 A_SetUserVar("user_headMinHeight", 45.0)
	PXCN A 0 A_SetUserVar("user_headMaxHeight", height)
	PXCN A 0 A_SetUserVar("user_legRadius", radius)
	PXCN A 0 A_SetUserVar("user_legMinHeight", 0)
	PXCN A 0 A_SetUserVar("user_legMaxHeight", 22.0)
	PXCN A 0 A_TakeInventory("BombAttempt",1)
	PXCN A 0 A_TakeInventory("Buy",1)
	PXCN A 1
    Goto Spawn+1
	See:
	PXCN ABCD 4
    Loop
	Missile:
    PXCN F 0 A_GiveInventory("IsFiring",1)
  	PXCN E 12
	PXCN F 0 A_TakeInventory("IsFiring",1)
    Goto Spawn
	Melee:
	PXCN F 6
    Goto Missile
  Pain:
    PXCN G 4 //A_JumpIfInventory("HeadHit",1,"HeadPain")
    PXCN G 4 A_Pain
    Goto Spawn
	Pain.Falling:
	PXCN G 4 A_PlaySound("misc/grunt",CHAN_VOICE)
	Goto Spawn
	Death:
	PXCN H 0 A_GiveInventory("IsDead",1)
    PXCN H 0 A_PlayerSkinCheck("AltSkinDeath")
	Death1:
    PXCN H 10
    PXCN I 10 A_PlayerScream
    PXCN J 10 A_NoBlocking
    PXCN KLM 10
    PXCN N -1
    Stop
	XDeath:
	PXCN H 0 A_GiveInventory("IsDead",1)
    PXCN O 0 A_PlayerSkinCheck("AltSkinXDeath")
	XDeath1:
    PXCN O 5
    PXCN P 5 A_XScream
    PXCN Q 5 A_NoBlocking
    PXCN RSTUV 5
    PXCN W -1
    Stop
	AltSkinDeath:
    PXCN H 6 
    PXCN I 6 A_PlayerScream
    PXCN JK 6
    PXCN L 6 A_NoBlocking
    PXCN MNO 6
    PXCN P -1
    Stop
	AltSkinXDeath:
    PXCN Q 5 A_PlayerScream
    PXCN R 0 A_NoBlocking
    PXCN R 5 A_SkullPop
    PXCN STUVWX 5
    PXCN Y -1
    Stop
  }
}
