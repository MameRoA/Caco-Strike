script "DeagleFire" (VOID)
{
   int x = GetActorVelX(0);
   int y = GetActorVelY(0);
   int z = GetActorZ(0) - GetActorFloorZ(0);
   int speed = FixedMul(x, x) + FixedMul(y, y);
   int velo = FixedSqrt(speed)/100000;
   
	if (velo >= 2 || z > 2.0 && CheckInventory("DeagleRecoil") == 0)
	{
	GiveInventory("DeagleRecoil",1);
	LineAttack(0,GetActorAngle(0) + random(FixedDiv(-2.1, 360.0),FixedDiv(2.1, 360.0)),GetActorPitch(0) + random(FixedDiv(-2.1, 360.0),FixedDiv(2.1, 360.0)),46,"CSPuff","Normal",range,FHF_NORANDOMPUFFZ);
    }
	else if (velo >= 2 || z > 2.0 && CheckInventory("DeagleRecoil") == 1)
	{
	LineAttack(0,GetActorAngle(0) + random(FixedDiv(-2.1, 360.0),FixedDiv(2.1, 360.0)),GetActorPitch(0) + random(FixedDiv(-2.1, 360.0),FixedDiv(2.1, 360.0)),46,"CSPuff","Normal",range,FHF_NORANDOMPUFFZ);
	}
	else if (velo < 2 && CheckInventory("DeagleRecoil") == 1)
	{
	LineAttack(0,GetActorAngle(0) + random(FixedDiv(-1.75, 360.0),FixedDiv(1.75, 360.0)),GetActorPitch(0) + random(FixedDiv(-1.75, 360.0),FixedDiv(1.75, 360.0)),46,"CSPuff","Normal",range,FHF_NORANDOMPUFFZ);
	}
	else
	{
    GiveInventory("DeagleRecoil",1);
	LineAttack(0,GetActorAngle(0) + random(FixedDiv(-0.3, 360.0),FixedDiv(0.3, 360.0)),GetActorPitch(0) + random(FixedDiv(-0.3, 360.0),FixedDiv(0.3, 360.0)),46,"CSPuff","Normal",range,FHF_NORANDOMPUFFZ);	
	}
	
}
